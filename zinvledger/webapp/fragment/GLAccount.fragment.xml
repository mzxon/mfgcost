<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="{i18n>GLAccount}"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		afterClose=".onValueHelpAfterClose"
		supportRanges="false"
		key="GLAccount"
		descriptionKey="GLAccountText">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onVHFBGL" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="FB_GROUP"
						name="GLAccount"
						label="{i18n>GLAccount}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="GLAccount" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="FB_GROUP"
						name="GLAccountText"
						label="{i18n>GLAccountText}"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="GLAccountText" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
