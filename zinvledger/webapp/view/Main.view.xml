    xmlns:l="sap.ui.layout"
    xmlns:f="sap.f"
    xmlns:table="sap.ui.table"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    xmlns:svm="sap.ui.comp.smartvariants"
    xmlns:mdc="sap.ui.mdc"
    xmlns:mdcField="sap.ui.mdc.field"
