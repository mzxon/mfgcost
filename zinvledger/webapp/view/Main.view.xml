    xmlns:form="sap.ui.layout.form"     
	xmlns:u="sap.ui.unified"
>
    <f:DynamicPage id="page" title="{i18n>title}" headerExpanded="{/headerExpanded}">
        <f:header>
            <f:DynamicPageHeader>
                <f:content>
                    <fb:FilterBar id="FB_V" search=".onSearch" showRestoreButton="true" 
                        useToolbar="false" showFilterConfiguration="false" >
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem 
                                groupName="FB_GROUP" name="CompanyCode" label="{i18n>CompanyCode}" 
                                labelTooltip="{i18n>CompanyCode}" mandatory="true" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox
                                        id="CB_CompanyCode"
                                        busyIndicatorDelay="0"
                                        showClearIcon="true"
                                        selectedKeys="{FB_Model>/CompanyCode}"
                                        showSecondaryValues="true"
                                        items="{
                                            path: '/VH_Company'
                                        }">
                                        <core:ListItem key="{CompanyCode}" text="{CompanyCodeName}" additionalText="{CompanyCode}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem 
                                groupName="FB_GROUP" name="FromDate" label="{i18n>FromDate}"
                                labelTooltip="{i18n>FromDate}" mandatory="true" visibleInFilterBar="true">
                                <fb:control>
                                    <DatePicker
                                        id="FromDate"
                                        required="true"
                                        value="{
                                            'path':'FB_Model>/FromDate',
                                            'type':'sap.ui.model.type.Date',
                                            'formatOptions': {
                                                'pattern': 'y-MM'
                                            }
                                        }"
                                        showCurrentDateButton="true"/>
                                </fb:control>
                            </fb:FilterGroupItem>           
                            <fb:FilterGroupItem
                                groupName="FB_GROUP" name="ToDate" label="{i18n>ToDate}"
                                labelTooltip="{i18n>FromDate}" mandatory="true" visibleInFilterBar="true">
                                <fb:control>
                                    <DatePicker
                                        id="ToDate"
                                        required="true"
                                        value="{
                                                'path': 'FB_Model>/ToDate',
                                                'type': 'sap.ui.model.type.Date',
                                                'formatOptions': {
                                                    'pattern': 'yyyy-MM'
                                                }
                                        }"
                                    />
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem 
                                groupName="FB_GROUP" name="Plant" label="{i18n>Plant}" 
                                labelTooltip="{i18n>Plant}" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiComboBox
                                        id="CB_Plant"
                                        busyIndicatorDelay="0"
                                        showClearIcon="true"
                                        selectedKeys="{FB_Model>/Plant}"
                                        showSecondaryValues="true"
                                        items="{
                                            path: '/VH_Plant'
                                        }">
                                        <core:ListItem key="{Plant}" text="{PlantName}" additionalText="{Plant}" />
                                    </MultiComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem 
                                groupName="FB_GROUP" name="Material" label="{i18n>Material}" 
                                labelTooltip="{i18n>Material}" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput
                                        id="MI_MT"
                                        width="60%"
                                        valueHelpRequest=".onVHMT"
                                        liveChange="onLiveChange"
                                        suggestionRows="{
                                            path: 'oMaterial>/'
                                        }">
                                        <suggestionRows>
                                            <ColumnListItem>
                                                <Label text="{oMaterial>Material}" />
                                                <Label text="{oMaterial>MaterialText}" />
                                            </ColumnListItem>
                                        </suggestionRows>
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                            <fb:FilterGroupItem 
                                groupName="FB_GROUP" name="GLAccount" label="{i18n>GLAccount}" 
                                labelTooltip="{i18n>GLAccount}" visibleInFilterBar="true">
                                <fb:control>
                                    <MultiInput
                                        id="MI_GL"
                                        width="60%"
                                        valueHelpRequest=".onVHGL"
                                        liveChange="onLiveChange"
                                        suggestionRows="{
                                            path: 'oGLAccount>/'
                                        }">
                                        <suggestionRows>
                                            <ColumnListItem>
                                                <Label text="{oGLAccount>GLAccount}" />
                                                <Label text="{oGLAccount>GLAccountText}" />
                                            </ColumnListItem>
                                        </suggestionRows>
                                    </MultiInput>
                                </fb:control>
                            </fb:FilterGroupItem>
                        </fb:filterGroupItems>
                    </fb:FilterBar>
