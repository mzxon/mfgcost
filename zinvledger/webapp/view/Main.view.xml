                                        <Text text="{oResult>EndQty}" wrapping="false" />        
                                    </HBox>
                                </table:template>
                            </table:Column>
                            <table:Column width="150px" hAlign="Center" headerSpan="3">
                                <table:multiLabels>
                                    <Label text="{i18n>End}" textAlign="Center" width="100%"/>
                                    <Label text="{i18n>Price}" textAlign="Center" width="100%"/>
                                </table:multiLabels>
                                <table:template>
                                    <HBox justifyContent="Start">
                                        <Text text="{
                                                parts: ['oResult>EndUnitPrice', 'oResult>CompanyCodeCurrency'], 
                                                type:'sap.ui.model.type.Currency',
                                                formatOptions: {
                                                    showMeasure: false
                                                }
                                            }" wrapping="false" />        
                                    </HBox>
                                </table:template>
                            </table:Column>
                            <table:Column width="150px" hAlign="Center" headerSpan="3">
                                <table:multiLabels>
                                    <Label text="{i18n>End}" textAlign="Center" width="100%"/>
                                    <Label text="{i18n>Amount}" textAlign="Center" width="100%"/>
                                </table:multiLabels>
                                <table:template>
                                    <HBox justifyContent="Start">
                                        <Text text="{
                                                parts: ['oResult>EndAmount', 'oResult>CompanyCodeCurrency'], 
                                                type:'sap.ui.model.type.Currency',
                                                formatOptions: {
                                                    showMeasure: false
                                                }
                                            }" wrapping="false" />        
                                    </HBox>  
                                </table:template>
                            </table:Column>      
                        </table:columns>
                    </table:Table>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
    </f:DynamicPage>
</mvc:View>
